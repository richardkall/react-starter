{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "analyze": "webpack --config webpack/webpack.client.babel.js --profile --json > stats.json",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --progress --config webpack/webpack.client.babel.js",
    "build:server": "webpack --progress --config webpack/webpack.server.babel.js",
    "dev": "npm run build:server && node build/server",
    "lint": "eslint --ignore-path .gitignore .",
    "prebuild": "rimraf build/*",
    "start": "node build/server"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "eslint": "3.14.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2"
  },
  "dependencies": {
    "apollo-client": "0.8.2",
    "assets-webpack-plugin": "3.5.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-react-remove-prop-types": "0.3.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "compression": "1.6.2",
    "eslint-plugin-react": "6.9.0",
    "express": "4.14.1",
    "file-loader": "0.10.0",
    "graphql-anywhere": "2.1.0",
    "graphql-tag": "1.2.4",
    "isomorphic-fetch": "2.2.1",
    "morgan": "1.8.0",
    "react": "15.4.2",
    "react-apollo": "0.10.0",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router-dom": "4.0.0-beta.4",
    "redux": "3.6.0",
    "rimraf": "2.5.4",
    "styled-components": "2.0.0-1",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-hot-middleware": "2.16.1",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  }
}
