{
  "name": "react-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env NODE_ENV=production webpack --config webpack/webpack.client.babel.js --profile --json > stats.json",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --progress --config webpack/webpack.client.babel.js",
    "build:server": "webpack --progress --config webpack/webpack.server.babel.js",
    "dev": "cross-env NODE_ENV=development npm run build:server && node build/server",
    "lint": "eslint --ignore-path .gitignore .",
    "prebuild": "rimraf build/*",
    "start": "cross-env NODE_ENV=production npm run build && node build/server"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "eslint": "3.14.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2"
  },
  "dependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-react-remove-prop-types": "0.2.11",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "compression": "1.6.2",
    "cross-env": "3.1.4",
    "eslint-plugin-react": "6.9.0",
    "express": "4.14.0",
    "file-loader": "0.10.0",
    "morgan": "1.7.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-router": "4.0.0-alpha.6",
    "rimraf": "2.5.4",
    "styled-components": "1.4.2",
    "url-loader": "0.5.7",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  }
}
